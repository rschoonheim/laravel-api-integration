{
    "name": "rschoonheim/laravel-api-integration-boilerplate",
    "description": "Boiler plate for integrating services into your Laravel project.",
    "type": "library",
    "authors": [
        {
            "name": "Romano Schoonheim",
            "email": "romano@romanoschoonheim.nl"
        }
    ],
    "require": {
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "johnkary/phpunit-speedtrap": "^4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rschoonheim\\LaravelApiIntegrationBoilerplate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rschoonheim\\LaravelApiIntegrationBoilerplate\\LaravelApiIntegrationServiceProvider"
            ]
        }
    }
}
